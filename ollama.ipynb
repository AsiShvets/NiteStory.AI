   "source": [
    "import requests\n",
    "\n",
    "def get_ollama_response(prompt, model=\"llama2\"):\n",
    "    url = f\"http://localhost:11434/api/completions\"\n",
    "    payload = {\"model\": model, \"prompt\": prompt}\n",
    "    response = requests.post(url, json=payload)\n",
    "    return response.json()[\"completion\"]\n"
   ]
  }
